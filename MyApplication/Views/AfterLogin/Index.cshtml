@model dynamic

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script>
        $(document).ready(function() {
            $('#queryButton').on('click', function() {
                $.ajax({
                       method: 'POST',
                       data: { },
                       url: 'AfterLogin/QueryApi',
                       success: function (data) {
                           let parse = JSON.parse(data);
                           debugger;
                           $('#result').empty().append(`Name: ${parse.name}, Age: ${parse.age}`);
                       },
                       error: function () {
                           reject('error');
                       }
                   });
            });
        }); 
    </script>
</head>
<body>
<div>
    <input type="button" value="Query" id="queryButton"/> 
    <div id="result"></div>
</div>
</body>
</html>